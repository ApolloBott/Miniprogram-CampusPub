/* 水平间距 */
.my-search-container {
  height: 40px;
  padding: 0 10px;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.my-search-box {
  height: 36px;
  background-color: #ffffff;
  border: 1px solid #E0E0E0;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.my-search-box .placeholder {
  font-size: 15px;
  margin-left: 5px;
}
