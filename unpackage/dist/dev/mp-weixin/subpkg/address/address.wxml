<view class="edit-address-page"><view class="form-container"><view class="form-item address-item"><text class="label required">详细地址</text><textarea class="textarea" placeholder="街道、楼牌号等详细信息" placeholder-class="placeholder" maxlength="200" auto-height="{{true}}" data-event-opts="{{[['input',[['__set_model',['$0','street','$event',[]],['formData']]]]]}}" value="{{formData.street}}" bindinput="__e"></textarea></view><view class="form-item switch-item"><text class="label">设为默认地址</text><switch checked="{{formData.is_default}}" color="#07c160" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view></view><view data-event-opts="{{[['tap',[['saveAddress',['$event']]]]]}}" class="save-btn" bindtap="__e"><text>保存</text></view><picker class="vue-ref" mode="region" value="{{regionValue}}" data-ref="regionPicker" data-event-opts="{{[['change',[['regionChange',['$event']]]]]}}" bindchange="__e"><view></view></picker></view>